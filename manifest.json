{
    "manifest_version": 2,
    "name": "Avoid Destraction",
    "version": "1.0",
    "description": "first Extension",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["Content.js"]
        }
      ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "management"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }
  